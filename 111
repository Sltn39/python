s = input()
index = 0
if s == "": print("пустая строка")
else:
    n, k = "", ""
    for i in range(len(s)):
        if s[i] in n:
            if len(k) <= len(n):
                k = n             
                index = i
            n = ""
        n += s[i]
        if len(k) <= len(n) and (i == len(s) - 1):
            k = n
    for j in range(index, 0, -1):
        if s[j] not in k: 
            k = s[j] + k
    print(k)   
